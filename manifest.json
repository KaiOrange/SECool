{
    "manifest_version": 2,
    "name": "SECool",
    "version": "1.0",
    "description": "使用搜索引擎时出现一些酷酷的效果。",
    "icons": {
        "16": "images/SECool_16.png",
        "48": "images/SECool_48.png"
    },
    "content_scripts": [
        {
            "matches": ["*://www.baidu.com/*","*://www.sogou.com/*",
                "*://www.so.com/*","*://cn.bing.com/*",
                "*://www.google.com/*","*://www.google.ae/*"],
            "css": ["css/animate.css","css/style.css"],
            "js": ["js/jquery.min.js","js/index.js"]
        }
    ]
}